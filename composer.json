  "authors": [
    {
      "name": "UmanIT",
      "email": "contact@umanit.fr"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "symfony/framework-bundle": ">=4.4",
    "liip/imagine-bundle": "~2.3"
  },
  "suggest": {
    "artgris/filemanager-bundle": "Artgris file manager"
  },
  "minimum-stability": "stable",
  "autoload": {
